{
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended",
    "plugin:css-modules/recommended"
  ],
  "plugins": ["prettier", "css-modules"],
  "rules": {
    "prettier/prettier": [
      "error",
      {},
      {
        "usePrettierrc": true
      }
    ],
    "no-console": ["error", { "allow": ["error"] }]
  }
}
